local Codes = {
    "OGHYPE",
    "REINDEER2023",
    "CANDYCANE2023",
    "SNOWMAN2023",
    "KART",
    "THANKSGIVING23",
    "HALLOWEEN2023",
    "DAUP", 
    "MELONBEAR",
    "pumpkins2023",
    "TRADEME!",
    "XMAS2023",
    "NEWYEAR2024",
    "PORTAL"
}



local remote = game.ReplicatedStorage.MainEvent

for _, code in ipairs(Codes) do
    print(code)
    remote:FireServer("EnterPromoCode", code)
    wait(4)
end
