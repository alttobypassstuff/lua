local Codes = {
    "SNOWMAN2023",
    "REINDEER2023",
    "CANDYCANE2023",
    "SNOWMAN2023",
    "KART",
    "THANKSGIVING23",
    "HALLOWEEN2023",
    "DAUP", 
    "MELONBEAR",
}

local remote = game.ReplicatedStorage.MainEvent

for _, code in ipairs(Codes) do
    print(code)
    remote:FireServer("EnterPromoCode", code)
    wait(2)
end
